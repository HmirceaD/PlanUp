<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <GridLayout
        android:id="@+id/icon_grid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true">

        <ImageButton
            android:id="@+id/car_icon"
            android:layout_width="75dp"
            android:layout_height="50dp"
            android:background="#FFF"
            android:src="@drawable/car_icon" />

        <ImageButton
            android:id="@+id/food_icon"
            android:layout_width="75dp"
            android:layout_height="50dp"
            android:layout_column="1"
            android:layout_row="0"
            android:background="#FFF"
            android:src="@drawable/food_icon" />

        <ImageButton
            android:id="@+id/house_icon"
            android:layout_width="75dp"
            android:layout_height="50dp"
            android:layout_column="2"
            android:layout_row="0"
            android:background="#FFF"
            android:src="@drawable/house_icon" />

        <ImageButton
            android:id="@+id/internet_icon"
            android:layout_width="75dp"
            android:layout_height="50dp"
            android:layout_column="0"
            android:layout_row="1"
            android:background="#FFF"
            android:src="@drawable/internet_icon" />

        <ImageButton
            android:id="@+id/phone_icon"
            android:layout_width="75dp"
            android:layout_height="50dp"
            android:layout_column="1"
            android:layout_row="1"
            android:background="#FFF"
            android:src="@drawable/phone_icon" />

        <ImageButton
            android:id="@+id/game_icon"
            android:layout_width="75dp"
            android:layout_height="50dp"
            android:layout_column="2"
            android:layout_row="1"
            android:background="#FFF"
            android:src="@drawable/game_icon" />

        <ImageButton
            android:id="@+id/gift_icon"
            android:layout_width="75dp"
            android:layout_height="50dp"
            android:layout_column="0"
            android:layout_row="2"
            android:background="#FFF"
            android:src="@drawable/gift_icon" />

        <ImageButton
            android:id="@+id/cart_icon"
            android:layout_width="75dp"
            android:layout_height="50dp"
            android:layout_column="1"
            android:layout_row="2"
            android:background="#FFF"
            android:src="@drawable/cart_icon" />

        <ImageButton
            android:id="@+id/shirt_icon"
            android:layout_width="75dp"
            android:layout_height="50dp"
            android:layout_column="2"
            android:layout_row="2"
            android:background="#FFF"
            android:src="@drawable/shirt_icon" />

        <ImageButton
            android:id="@+id/dress_icon"
            android:layout_width="75dp"
            android:layout_height="50dp"
            android:layout_column="0"
            android:layout_row="3"
            android:background="#FFF"
            android:src="@drawable/dress_icon" />

        <ImageButton
            android:id="@+id/shoes_icon"
            android:layout_width="75dp"
            android:layout_height="50dp"
            android:layout_column="1"
            android:layout_row="3"
            android:background="#FFF"
            android:src="@drawable/shoes_icon" />

        <ImageButton
            android:id="@+id/travel_icon"
            android:layout_width="75dp"
            android:layout_height="50dp"
            android:layout_column="2"
            android:layout_row="3"
            android:background="#FFF"
            android:src="@drawable/travel_icon" />

        <ImageButton
            android:id="@+id/utilities_icon"
            android:layout_width="75dp"
            android:layout_height="50dp"
            android:layout_column="0"
            android:layout_row="4"
            android:background="#FFF"
            android:src="@drawable/utilities_icon" />

        <ImageButton
            android:id="@+id/wifi_icon"
            android:layout_width="75dp"
            android:layout_height="50dp"
            android:layout_column="1"
            android:layout_row="4"
            android:background="#FFF"
            android:src="@drawable/wifi_icon" />

        <ImageButton
            android:id="@+id/repair_icon"
            android:layout_width="75dp"
            android:layout_height="50dp"
            android:layout_column="2"
            android:layout_row="4"
            android:background="#FFF"
            android:src="@drawable/repair_icon" />

    </GridLayout>
</RelativeLayout>